train_net: "train_val.prototxt"
display: 100
lr_policy: "step"
# lr for unnormalized softmax
base_lr:1e-04
gamma: 0.1
stepsize: 25000
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 50000
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "train"
test_initialization: false
